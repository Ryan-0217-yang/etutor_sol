n = int(input())
ch = []
en = []
ma = []
total = []

for i in range(n):
    a , b , c =map(int,input().split())
    ch.append(a)
    en.append(b)
    ma.append(c)

avgch = sum(ch)/n
avgen = sum(en)/n
avgma = sum(ma)/n
avgall = (avgch + avgen + avgma) / 3

print(f"{avgall:.1f} {avgch:.1f} {avgen:.1f} {avgma:.1f}")
